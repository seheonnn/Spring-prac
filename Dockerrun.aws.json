{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": {
    "springboot-app": {
      "image": "seheon/beanstalk-docker",
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        },
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        },
        {
          "name": "REDIS_HOST",
          "value": "redis"
        }
      ],
      "dependsOn": [
        "redis"
      ]
    },
    "redis": {
      "image": "redis",
      "memory": 256,
      "essential": false,
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379
        }
      ]
    }
  }
}
