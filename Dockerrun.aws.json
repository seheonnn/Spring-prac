{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "beanstalk-app",
      "image": "891377099059.dkr.ecr.ap-northeast-2.amazonaws.com/beanstalk",
      "essential": true,
      "ports": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "essential": true,
      "ports": [
        {
          "containerPort": 6379,
          "hostPort": 6379
        }
      ]
    }
  ]
}
