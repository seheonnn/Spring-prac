{
  "AWSEBDockerrunVersion": "1",
  "Volumes": [],
  "Containers": [
    {
      "Name": "springboot-app",
      "Image": "seheon/beanstalk-docker",
      "Essential": true,
      "Memory": 512,
      "Ports": [
        {
          "HostPort": 8080,
          "ContainerPort": 8080
        }
      ],
      "Environment": [
        {
          "Name": "SPRING_PROFILES_ACTIVE",
          "Value": "dev"
        },
        {
          "Name": "TZ",
          "Value": "Asia/Seoul"
        }
      ]
    }
  ]
}
