{
  "AWSEBDockerrunVersion": "1",
  "Volumes": [],
  "Containers": [
    {
      "Name": "beanstalk-docker",
      "Image": "seheon/beanstalk-docker",
      "Essential": true,
      "Memory": 512,
      "PortMappings": [
        {
          "HostPort": 8080,
          "ContainerPort": 8080
        }
      ],
      "Ports": [
        {
          "ContainerPort": 8080
        }
      ]
    },
    {
      "Name": "redis",
      "Image": "redis",
      "Essential": false,
      "Memory": 256,
      "PortMappings": [
        {
          "HostPort": 6379,
          "ContainerPort": 6379
        }
      ],
      "Ports": [
        {
          "ContainerPort": 6379
        }
      ]
    }
  ]
}
