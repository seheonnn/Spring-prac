{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "springboot-app",
      "image": "seheon/beanstalk-docker",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        },
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        },
        {
          "name": "REDIS_HOST",
          "value": "redis"
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "redis",
      "image": "redis",
      "essential": false,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    }
  ]
}
