{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "springboot-app",
      "image": "891377099059.dkr.ecr.ap-northeast-2.amazonaws.com/beanstalk:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        },
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        },
        {
          "name": "REDIS_HOST",
          "value": "redis"
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "essential": false,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    }
  ]
}
