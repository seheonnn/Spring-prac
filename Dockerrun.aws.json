{
  "AWSEBDockerrunVersion": "1",
  "ContainerDefinitions": [
    {
      "Name": "springboot-app",
      "Image": "891377099059.dkr.ecr.ap-northeast-2.amazonaws.com/beanstalk",
      "Essential": true,
      "Memory": 512,
      "Ports": [
        {
          "ContainerPort": 8080,
          "HostPort": 8080
        }
      ],
      "Environment": [
        {
          "Name": "REDIS_HOST",
          "Value": "redis"
        }
      ],
      "Links": [
        "redis"
      ]
    },
    {
      "Name": "redis",
      "Image": "redis:latest",
      "Essential": false,
      "Memory": 256,
      "Ports": [
        {
          "ContainerPort": 6379,
          "HostPort": 6379
        }
      ]
    }
  ]
}
