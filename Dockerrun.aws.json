{
  "AWSEBDockerrunVersion": "1",
  "ContainerDefinitions": [
    {
      "Name": "springboot-app",
      "Image": "seheon/beanstalk-docker",
      "Update": "true",
      "PortMappings": [
        {
          "ContainerPort": 8080,
          "HostPort": 8080
        }
      ],
      "Environment": [
        {
          "Name": "SPRING_PROFILES_ACTIVE",
          "Value": "dev"
        },
        {
          "Name": "TZ",
          "Value": "Asia/Seoul"
        },
        {
          "Name": "REDIS_HOST",
          "Value": "redis"
        }
      ],
      "Links": [
        "redis"
      ]
    },
    {
      "Name": "redis",
      "Image": "redis",
      "PortMappings": [
        {
          "ContainerPort": 6379,
          "HostPort": 6379
        }
      ]
    }
  ]
}
